# Environment variables for Hardhat development

# Private key for deployment (never commit the actual private key)
PRIVATE_KEY=your_private_key_here

# Sepolia testnet RPC URL (get from Alchemy or Infura)
SEPOLIA_URL=https://eth-sepolia.g.alchemy.com/v2/your_api_key_here

# Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# NFT.storage API key for IPFS uploads
NFT_STORAGE_API_KEY=your_nft_storage_api_key_here